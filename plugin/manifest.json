{
    "name": "Current tab plugin",
    "version": "1.0.0",
    "description": "Current tab plugin",
    "manifest_version": 3,
    "author": "Bo ja na złość im nie należę i tak przeze mnie o mnie gra. Jednego nikt mi nie odbierze, ja jestem ja.",
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
          "matches": ["http://localhost:5812/*"],
          "js": ["content.js"]
        }
    ]
}